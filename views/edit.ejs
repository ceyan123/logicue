<head>
    <%- include('partial/head'); %>
</head>

<body id="landing" class="sidebar-open">

<div class="page-container animsition">
    <div id="dashboardPage">
        

<!-- Header -->
<%- include('partial/header'); %>

<!-- SideBar -->
<%- include('partial/sidebar'); %>

<main>
    <div class="page-breadcrumb">
        <div class="row">
            <div class="col-6">
                <h4 class="page-title">Edit Product</h4>
                <nav aria-label="breadcrumb">
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item"><a href="/">Home</a></li>
                        <li class="breadcrumb-item active" aria-current="page">Edit Product</li>
                    </ol>
                </nav>
            </div>
            <div class="col-6">
                <div class="text-right">
                    <small>Users Online</small>
                    <h5 class="text-info">3,458</h5>
                </div> 
            </div>
        </div>
    </div>

    <div class="container-fluid">

        <%- include('partial/star'); %>
        <div>
            <form action="/update/<%= data._id %>" method="post" >
                <div class="row">
                    <div class="col-sm-3">
                        <label for="">Nama Barang</label> <input type="text" value="<%= data.namabarang %>" id="namabarang" name="namabarang" required>
                    </div>
                    <div class="col-sm-3">
                        <label for="">Harge Beli</label> <input type="number" value="<%= data.hargabeli %>" id="hargabeli" name="hargabeli" required>
                    </div>
                    <div class="col-sm-3">
                        <label for="">Harge Jual</label> <input type="number" value="<%= data.hargajual %>" id="hargajual" name="hargajual" required>
                    </div>
                    <div class="col-sm-3">
                        <label for="">Stok</label> <input type="number" value="<%= data.stok %>" id="stock" name="stok" required>
                    </div>
                    <div class="col-sm-3">
                        <label for="">Image</label> <input type="text" value="<%= data.image %>" id="image" name="image" disabled>
                    </div>
                </div style="display: flex; justify-content: center; ">
                <div class="row">
                    <div class="col-sm-6" style="margin-top:40px;">
                        <button type="submit" style="align-items: center;
                        background-image: linear-gradient(144deg,#AF40FF, #5B42F3 50%,#00DDEB);
                        border: 0;
                        border-radius: 8px;
                        box-shadow: rgba(151, 65, 252, 0.2) 0 15px 30px -5px;
                        box-sizing: border-box;
                        color: #FFFFFF;
                        display: flex;
                        font-family: Phantomsans, sans-serif;
                        font-size: 20px;
                        justify-content: center;
                        line-height: 1em;
                        max-width: 100%;
                        min-width: 140px;
                        padding: 19px 24px;
                        text-decoration: none;
                        user-select: none;
                        -webkit-user-select: none;
                        touch-action: manipulation;
                        white-space: nowrap;
                        cursor: pointer;">Edit Product</button>
                    </div>
                </div>
            </form>
        </div>

        
    </div>


    <!-- Footer -->
    <%- include('partial/footer'); %>

</main>
</div>
</div>



